{
  "name": "php-electron-vue",
  "private": true,
  "version": "0.0.0",
  "main": "src/electron.js",
  "scripts": {
    "vite": "vite",
    "electron": "electron .",
    "dev": "vite & electron .",
    "build": "vite build",
    "build-ts": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
